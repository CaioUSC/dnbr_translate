   <talk_paragraph index="n393_academic_station-1-a" image="QN_Npc_Academic_jasmine.dds" itemindex="">
      <talk_answer link_index="n393_academic_station-1-b" mid="12"><![CDATA[<html>(Continue)</html>]]></talk_answer>
      <talk_question mid="11"><![CDATA[<html>$Academic Jasmine$<BR>It would be great if you brought him some medicine.<BR>But how would we know what to use to treat dragons?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n393_academic_station-1-b" image="QN_Npc_Academic_jasmine.dds" itemindex="">
      <talk_answer link_index="n393_academic_station-1-c" mid="14"><![CDATA[<html>(Try to remember) ...Ahh!</html>]]></talk_answer>
      <talk_question mid="13"><![CDATA[<html>$Academic Jasmine$<BR>Well, you talked to him for quite a bit.<BR>Some data... Hmm, so did you hear anything useful?</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n393_academic_station-1-c" image="QN_Npc_Academic_jasmine.dds" itemindex="">
