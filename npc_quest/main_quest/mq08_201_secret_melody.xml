   <talk_paragraph index="n023_ranger_fugus-1-a" image="QW_ManaRidge.dds" itemindex="">
      <talk_answer link_index="n023_ranger_fugus-1-b" mid="10"><![CDATA[<html>(Continue)</html>]]></talk_answer>
