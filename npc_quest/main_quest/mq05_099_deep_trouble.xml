   <talk_paragraph index="n012_sorceress_master_cynthia-accepting" image="" itemindex="">
      <talk_answer link_index="n012_sorceress_master_cynthia-accepting-acceptted" mid="7"><![CDATA[<html>(Continue)</html>]]></talk_answer>
