   <talk_paragraph index="n012_sorceress_master_cynthia-4-b" image="" itemindex="">
      <talk_answer link_index="n012_sorceress_master_cynthia-4-c" mid="31"><![CDATA[<html>(Continue)</html>]]></talk_answer>
