   <talk_paragraph index="n464_empty_place-2" image="QN_Npc_Sidel.dds" itemindex="">
      <talk_answer link_index="n464_empty_place-2-a" mid="39"><![CDATA[<html>(Continue)</html>]]></talk_answer>
      <talk_question mid="38"><![CDATA[<html>$Cidel$<BR>These Natives' condition is worse than the ones who got rescued the last time. I have to take care of them first. Ask Master how to cure them.</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n464_empty_place-2-a" image="QN_Npc_karahan.dds" itemindex="">
