      <talk_question mid="1987"><![CDATA[<html>$퀘스트 보상$</html>]]></talk_question>
   </talk_paragraph>
   <talk_paragraph index="n088_scholar_starshy-5-g" image="" itemindex="" portrait_left="" portrait_right="NPC_Right_Starshy.dds" center_image="">
      <talk_answer link_index="n088_scholar_starshy-5-!next" mid="2011" select_type="2" />
