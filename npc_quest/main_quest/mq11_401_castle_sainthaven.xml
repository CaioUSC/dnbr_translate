         <trigger_string />
         <maptool_index />
         <title mid="25"><![CDATA[Operation Airship Nautilus]]></title>
         <contents mid="26"><![CDATA[<html><FONT color=#a82424></FONT>You have brought the Airship Engine Battery to Airship Engineer Hubert. Talk to Airship Engineer Hubert.<BR><FONT color=#7e12d2>※ You can move to Saint Haven once you reach level 24.</FONT><BR><FONT color=#086171></FONT><BR><FONT color=#086171>Calderock Village</FONT> <FONT color=#a82424>→ Airship Engineer Hubert</FONT></html>]]></contents>
         <destination mid="27"><![CDATA[Talk to Airship Engineer Hubert]]></destination>
         <todo_msg mid="28"><![CDATA[Quest has been added.]]></todo_msg>
      </journal>
      <journal index="2">
         <contents_image>QN_Npc_Woman_Tiketer_Arnene.dds</contents_image>
         <destination_pos>8,n69</destination_pos>
         <destination_mode>custom</destination_mode>
